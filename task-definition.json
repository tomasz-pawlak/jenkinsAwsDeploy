{
    "family": FAMILY,
    "containerDefinitions": [
        {
            "image": "REPOSITORY_URI:BUILD_NUMBER",
            "name": NAME,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": ROLE_ARN
}
