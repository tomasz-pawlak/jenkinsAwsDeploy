{
    "family": "spring2",
    "containerDefinitions": [
        {
            "image": "REPOSITORY_URI:BUILD_NUMBER",
            "name": "springRepo2",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ],
    "cpu": "1024",
    "memory": "2048",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::410958652748:role/ecsTaskExecutionRole"
}
